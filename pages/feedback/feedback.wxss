/* pages/feedback/feedback.wxss */
.content{
  width: 80%;
  height: 300rpx;
  margin: 40rpx auto;
  border: 2px solid rgb(0, 0, 0,0.3);
  border-radius: 10px;
}
.content textarea{padding: 10rpx;}
.tel{
  width: 60%;
  margin: 20rpx auto;
}
.tel input{
  width: 100%;
  height: 60rpx;
  margin-top: 20rpx;
  border: 2px solid rgb(0, 0, 0,0.3);
  border-radius: 10px;
  padding-left: 8rpx;
  font-size: smaller;
}
.sub{
  width: 200rpx;
  height: 70rpx;
  border-radius: 10px;
  background-color: blanchedalmond;
  margin: 20rpx auto;
  text-align: center;
  line-height: 70rpx;
}